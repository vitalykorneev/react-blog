  "betterScripts": {
    "start-dev": {
      "command": "node ./server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-prod": {
      "command": "node ./server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    }
  },
